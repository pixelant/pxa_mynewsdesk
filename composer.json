{
  "name": "pixelant/pxa-mynewsdesk",
  "description": "My news desk import",
  "type": "typo3-cms-extension",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Pixelant",
      "email": "info@pixelant.net",
      "homepage": "https://www.pixelant.net",
      "role": "Developer"
    }
  ],
  "require": {
    "typo3/cms-core": "^9.5",
    "php": ">=7.2"
  },
  "require-dev": {
    "georgringer/news": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Pixelant\\PxaMynewsdesk\\": "Classes/"
    }
  },
  "replace": {
    "typo3-ter/pxa-mynewsdesk": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "pxa_mynewsdesk"
    }
  }
}
